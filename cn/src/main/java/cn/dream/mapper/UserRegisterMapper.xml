<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.dream.mapper.UserRegisterMapper">
	<select id="queryUser" resultType="cn.dream.entity.UserRegister" parameterType="string">
		SELECT userAccount,passWord
		FROM user
		WHERE user.userAccount=#{value}
	</select>
	<insert id="addUser" parameterType="cn.dream.entity.UserRegister">
		INSERT INTO
		user(userAccount,passWord)
		VALUES
		(#{userAccount},#{passWord})
	</insert>
</mapper>